{
  "name": "@angela-1/primer-vue",
  "description": "A UI Toolkit built on Vue.js 3.0 with @primer/css",
  "version": "0.0.2",
  "author": "Angela-1",
  "repository": "https://github.com/angela-1/primer-vue.git",
  "license": "MulanPSL-2.0",
  "keywords": [
    "vuejs",
    "primer-css"
  ],
  "scripts": {
    "dev": "vite",
    "build:dev": "vite build",
    "build:prod": "yarn clean && vuedx-typecheck ./packages && vite build",
    "clean": "rm -rf dist",
    "publish:prod": "sh scripts/publish-to-npm.sh"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/primer-vue.umd.js",
  "module": "./dist/primer-vue.es.js",
  "exports": "./dist/primer-vue.es.js",
  "dependencies": {
    "@primer/css": "^15.2.0",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@vitejs/plugin-vue": "^1.0.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.5.0",
    "@vuedx/typescript-plugin-vue": "^0.5.0",
    "chalk": "^4.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.4",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.26",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  }
}
