{
  "name": "primer-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "prettier --write  \"packages/**/*.?(ts|vue)\"",
    "test": "jest"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --parser=typescript --write"
    ],
    "*.vue": [
      "prettier --parser=vue --write"
    ]
  },
  "dependencies": {
    "@primer/css": "^15.2.0",
    "@primer/octicons": "^11.0.0",
    "dayjs": "^1.9.4",
    "vue": "^3.0.2",
    "vue-router": "4.0.0-beta.4"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/test-utils": "^2.0.0-beta.7",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "sass": "^1.26.11",
    "vite": "^1.0.0-rc.6",
    "vitepress": "^0.5.0",
    "vue-jest": "^5.0.0-alpha.5",
    "yorkie": "^2.0.0"
  }
}
